package edu.cmu.gizmo.management.taskmanager.exceptions;


/**
 * Exception thrown when task configuration is incomplete.
 *
 * @version 1.0 2012-06-23
 * @author Jeff Gennari
 */
public class TaskConfigurationIncompleteException extends Exception {

	/**
	 * Create a new TaskConfigurationIncompleteException with a message.
	 *
	 * @param msg the exception message
	 */
	public TaskConfigurationIncompleteException(final String msg) {
		super(msg);
	}

}
