package edu.cmu.gizmo.management.taskmanager;


/**
 * The Class Tasks.
 */
class Tasks {
	
	/** The state. */
	private Object state;
	
	/** The timestamp. */
	private Float timestamp;
	
	/**
	 * Sets the state.
	 *
	 * @param state the new state
	 */
	public void setState(Object state) {
		this.state = state;
	}

	/**
	 * Gets the state.
	 *
	 * @return the state
	 */
	public Object getState() {
		return state;
	}
};