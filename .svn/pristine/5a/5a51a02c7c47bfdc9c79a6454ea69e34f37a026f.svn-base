package edu.cmu.gizmo.management.robot;


/**
 * 
 * This class represents an active command that the Cobot 3 is executing.
 * 
 * @version 1.0
 * @author jeff Gennari
 */
public class Cobot3Command {
	
	/** The cmd number. */
	private Integer cmdNumber;

	/**
	 * Get the unique number of the command.
	 *
	 * @return the unique number for this command
	 */
	public Integer getCmdNumber() {
		return cmdNumber;
	}

	/**
	 * set the command number.
	 *
	 * @param cmdNumber the unique number for this command. This number is maintained
	 * on the Management System and the Robot Manager.
	 */
	public void setCmdNumber(final Integer cmdNumber) {
		this.cmdNumber = cmdNumber;
	}
}
