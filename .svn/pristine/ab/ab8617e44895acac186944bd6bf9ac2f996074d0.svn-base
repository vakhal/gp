package edu.cmu.gizmo.management.taskmanager.scripttask;


/**
 * The Class PlannedTaskStatus.
 */
public class PlannedTaskStatus {
	
	/** The ret val. */
	private int retVal = -1;
	
	/** The msg. */
	private String msg = null;

	/**
	 * Gets the ret val.
	 *
	 * @return the ret val
	 */
	public int getRetVal() {
		return retVal;
	}
	
	/**
	 * Sets the ret val.
	 *
	 * @param retVal the new ret val
	 */
	public void setRetVal(int retVal) {
		this.retVal = retVal;
	}
	
	/**
	 * Gets the msg.
	 *
	 * @return the msg
	 */
	public String getMsg() {
		return msg;
	}
	
	/**
	 * Sets the msg.
	 *
	 * @param msg the new msg
	 */
	public void setMsg(String msg) {
		this.msg = msg;
	}
}
