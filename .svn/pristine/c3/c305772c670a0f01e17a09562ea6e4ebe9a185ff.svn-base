package edu.cmu.gizmo.management.beans;

import java.io.Serializable;

/**
 * CapabilityBean is a JavaBean of a Capability object orignially designed to represent a capability.
 * However, <code>Task</code> is created to incorporate CapabilityBean with a reference to a Capabilty object.
 * 
 * @author sjnicklee
 * @deprecated
 */
public class CapabilityBean implements Serializable {
	/** Capability ID **/
	private int cp_id;

	/** Capability Name**/
	private String cp_name;
	
	/** Capability Description **/
	private String cp_desc;

	/** Capability Type **/
	private String cp_typ_cd;

	/** Capability Execution type - Sequential or Parallel **/
	private String cp_exec_typ_cd;

	/**
	 * Returns capability Id value
	 * @return
	 */
	public int getCp_id() {
		return cp_id;
	}
	
	/**
	 * Sets capability Id value
	 * @param cp_id
	 */
	public void setCp_id(int cp_id) {
		this.cp_id = cp_id;
	}
	
	/**
	 * Returns capability name 
	 * @return
	 */
	public String getCp_name() {
		return cp_name;
	}
	
	/**
	 * Sets capability name
	 * @param cp_name
	 */
	public void setCp_name(String cp_name) {
		this.cp_name = cp_name;
	}
	
	/**
	 * Returns capability description
	 * @return
	 */
	public String getCp_desc() {
		return cp_desc;
	}
	
	/**
	 * Sets capability description
	 * @param cp_desc
	 */
	public void setCp_desc(String cp_desc) {
		this.cp_desc = cp_desc;
	}
	
	/**
	 * Returns capability type
	 * @return
	 */
	public String getCp_typ_cd() {
		return cp_typ_cd;
	}
	
	/**
	 * Sets capability type
	 * @param cp_typ_cd
	 */
	public void setCp_typ_cd(String cp_typ_cd) {
		this.cp_typ_cd = cp_typ_cd;
	}
	
	/**
	 * Returns capability execution type
	 * @return
	 */
	public String getCp_exec_typ_cd() {
		return cp_exec_typ_cd;
	}
	
	/**
	 * Sets capability execution type
	 * @param cp_exec_typ_cd
	 */
	public void setCp_exec_typ_cd(String cp_exec_typ_cd) {
		this.cp_exec_typ_cd = cp_exec_typ_cd;
	}
}
